{
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "React": "readonly",
        "ReactRouterDOM": "readonly",
        "Gorilla": "readonly",
        "QRious": "readonly"
    },
    "ignorePatterns": [
        "lib",
        "design-ideas"
    ],
    "rules": {
        "react/jsx-uses-vars": "error"
    }
}